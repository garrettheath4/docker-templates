<?xml version="1.0"?>
<Container version="2">
  <Name>tekkit-minecraft-server</Name>
  <Repository>garrettheath4/tekkit-minecraft</Repository>
  <Registry>https://hub.docker.com/r/garrettheath4/tekkit-minecraft/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/garrettheath4/tekkit-minecraft/</Support>
  <Project/>
  <Overview>Tekkit Normal v1.2.9i Docker container. Forked from LGDan/tekkit-classic-docker repository on GitHub.</Overview>
  <Category>GameServers:</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/garrettheath4/docker-templates/master/tekkit-logo.png</Icon>
  <ExtraParams>--log-opt max-size=50m --log-opt max-file=1</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Description>Tekkit Classic v3.1.2 Minecraft server Docker container. Forked from dang1t/tekkit-classic-docker repository on GitHub.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>25575</HostPort>
        <ContainerPort>25565</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/tekkit-minecraft-server/minecraft</HostDir>
      <ContainerDir>/minecraft</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>garrettheath4</Value>
      <Name>SERVER_OP</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Minecraft Port" Target="25565" Default="25565" Mode="tcp" Description="Container Port: 25565" Type="Port" Display="always" Required="true" Mask="false">25575</Config>
  <Config Name="Minecraft Account" Target="SERVER_OP" Default="" Mode="" Description="Container Variable: SERVER_OP" Type="Variable" Display="always" Required="true" Mask="false">garrettheath4</Config>
  <Config Name="Minecraft Data" Target="/minecraft" Default="/mnt/user/appdata/tekkit-minecraft-server/minecraft" Mode="rw" Description="Container Path: /minecraft" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/tekkit-minecraft-server/minecraft</Config>
</Container>
